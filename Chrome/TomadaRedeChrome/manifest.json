{
  "name": "Arduino Blink",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "description": "Show off serial functionality connecting to Arduino.",

  "app": {
    "background": {
      "scripts": [ "background.js" ]
    }
  },

  "icons": {
    "16": "assets/lamp.png",
    "128": "assets/lamp.png"
  },

  "permissions": [
    "serial"
  ]
}
